
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GymApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/GymApp.styles.css" asp-append-version="true" />
   
</head>
    <body>
        @await Component.InvokeAsync("Navbar")

        @RenderBody()

        <section id="footerPart">
            <div class="container">
                <footer class="">
                    <ul class="nav justify-content-center list-unstyled d-flex">
                        <li class="ms-3">
                            <a class="text-body-secondary" href="https://twitter.com/birollsahinn">
                                <img src="/images/x-icon2.png" alt="">
                            </a>
                        </li>
                        <li class="ms-3">
                            <a class="text-body-secondary" href="https://www.instagram.com/birollsahiinn/">
                                <img src="/images/icons8-instagram-50.png" alt="">
                            </a>
                        </li>
                        <li class="ms-3">
                            <a class="text-body-secondary" href="#">
                                <img src="/images/icons8-facebook-30.png" alt="">
                            </a>
                        </li>
                    </ul>
                    <hr>
                    <p class="text-center text-body-secondary">© 2023 IronSculpt GYM, Inc</p>
                </footer>
            </div>
        </section>


        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>
